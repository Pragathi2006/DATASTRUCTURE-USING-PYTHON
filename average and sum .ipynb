    "# FIND THE SUM AND AVERAGE OF AN ARRAY ELEMENT:-\n",
    "import numpy as np\n",
    "x=np.array([10,20,30,40,50])\n",
    "def avg(x):\n",
    "    l=len(x)\n",
    "    add_all=sum(x)\n",
    "    average=add_all/l\n",
    "    return average\n",
    "result=avg(x)\n",
