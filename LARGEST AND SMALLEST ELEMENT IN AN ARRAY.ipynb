   "source": [
    "# FIND THE LARGEST AND SMALLEST ELEMENT IN AN ARRAY:-\n",
    "import numpy as np\n",
    "x=np.array([10,20,30,40,50])\n",
    "def min_max(x):\n",
    "    min_e=np.min(x)\n",
    "    max_e=np.max(x)\n",
    "    return min_e,max_e\n",
    "result=min_max(x)\n",
    "print(result)"
   ]
